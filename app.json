{
  "expo": {
    "name": "BookBLA",
    "slug": "bookbla",
    "owner": "bookblakorea",
    "description": "BookBla application",
    "version": "0.1.2",
    "orientation": "portrait",
    "scheme": "bookbla",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/logos/Splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "usesAppleSignIn": true,
      "infoPlist": {
        "UIRequiresFullScreen": true,
        "CFBundleDevelopmentRegion": "ko"
      },
      "bundleIdentifier": "shop.bookbla",
      "buildNumber": "102",
      "icon": "./assets/images/logos/logoDarkBg.png",
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "icon": "./assets/images/logos/logoDarkBg.png",
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.bookbla.bookblaapp",
      "versionCode": 11
    },
    "notification": {
      "icon": "./assets/images/logos/Mdpi.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/logos/Mdpi.png"
        }
      ],
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      "@react-native-firebase/crashlytics",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "8017098d-c588-4b90-b1c5-c865ec67538e"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/8017098d-c588-4b90-b1c5-c865ec67538e"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
